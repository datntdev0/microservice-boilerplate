@using datntdev.Microservice.Identity.Web.Host.Models

@if (!string.IsNullOrEmpty(Options?.Title))
{
  <script>
    Swal.fire({
      title: '@Options?.Title',
      text: '@Options?.Text',
      icon: '@Options?.Icon',
    })
  </script>
}

@code {
  [Parameter] public SweetAlertModel? Options { get; set; }
}
